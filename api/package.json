{
  "name": "rest-101",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "concurrently --kill-others \"npm run watch\" \"nodemon --inspect ./dist/server.js\"",
    "migration:create": "npm run build && npx sequelize-cli migration:create --name",
    "db:migrate": "npm run build && npx sequelize-cli db:migrate",
    "migration:undo": "npm run build && npx sequelize-cli db:migrate:undo",
    "db:drop": "npm run build && npx sequelize-cli db:drop",
    "db:create": "npm run build && npx sequelize-cli db:create",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-json-validator-middleware": "^3.0.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.7",
    "@types/validator": "^13.7.17",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
