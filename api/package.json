{
  "name": "rest-101",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc && rm -rf dist/tests",
    "watch": "tsc -w",
    "start": "concurrently --kill-others \"npm run watch\" \"nodemon --inspect ./dist/src/server.js\"",
    "migration:create": "npm run build && npx sequelize-cli migration:create --name",
    "db:migrate": "npm run build && npx sequelize-cli db:migrate",
    "migration:undo": "npm run build && npx sequelize-cli db:migrate:undo",
    "db:drop": "npm run build && npx sequelize-cli db:drop",
    "db:create": "npm run build && npx sequelize-cli db:create",
    "test": "rm -rf dist/tests && cross-env NODE_ENV=test jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-json-validator-middleware": "^3.0.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@jest/globals": "^29.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.1.7",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "test-jest": "^1.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
